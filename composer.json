    "keywords": [
        "uri",
        "url",
        "urn",
        "oop-library"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Marezki",
            "email": "felix@yuubit.de",
            "homepage": "http://yuubit.de",
